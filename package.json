{
  "name": "str-data-frame",
  "version": "0.2.34",
  "description": "Utiltities to handle pandas like data frames",
  "main": "lib/index.js",
  "unpkg": "dist/",
  "scripts": {
    "build": "node code_spliter.node.js && babel src -d lib && rollup -c",
    "split": "node code_spliter.node.js",
    "rollup": "rollup -c",
    "pkg": "babel src -d lib && rollup -c && npm pack && mv data-frame-*.tgz ../npmlocal",
    "test": "mocha --compilers js:babel-core/register ./src/*.test.js ./src/**/*.test.js",
    "gen:doc": "esdoc",
    "gen:web": "esdoc -c esdoc-forweb.json",
    "docs": "open docs/index.html"
  },
  "author": "nurul choudhury",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/traverse": "^7.11.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/babel__core": "^7.1.9",
    "babel-minify": "^0.5.1",
    "babel-plugin-loop-optimizer": "^1.4.1",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-preset-minify": "^0.2.0",
    "chai": "^4.2.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^3.1.0",
    "fetch": "^1.1.0",
    "mocha": "^8.1.3",
    "plotly": "^1.0.6",
    "request": "^2.88.2",
    "rollup": "^2.23.1",
    "rollup-plugin-terser": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nurulc/data-frame.git"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,html,scss",
      "quiet": false
    }
  },
  "dependencies": {
    "esprima": "^4.0.1"
  }
}
