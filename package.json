{
  "name": "data-frame",
  "version": "0.1.0",
  "description": "Utiltities to handle pandas like data frames",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "rollup": "rollup -c",
    "pkg": "babel src -d lib && npm pack && mv data-frame-*.tgz ../npmlocal",
    "test": "mocha --compilers js:babel-core/register ./src/*.test.js ./src/**/*.test.js",
    "gen:doc": "esdoc",
    "docs": "open docs/index.html",
    "watch-build": "npm-watch build"
  },
  "author": "nurul choudhury",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/traverse": "^7.4.0",
    "ajv": "^5.0.0",
    "babel-plugin-loop-optimizer": "^1.4.1",
    "babel-preset-minify": "^0.2.0",
    "chai": "^4.2.0",
    "esdoc": "^1.0.1",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.5.3",
    "rollup": "^1.7.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-node-resolve": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.remedysystems.com:7999/ep/matcher.git"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,html,scss",
      "quiet": false
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "babel-minify": "^0.5.0"
  }
}
